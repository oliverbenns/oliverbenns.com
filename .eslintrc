{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
  },
  "extends": "airbnb",
  "rules": {
    "arrow-parens": 0,
    "global-require": [0],
    "max-len": [0],
    "import/extensions": 0,
    "import/imports-first": 0,
    "react/forbid-prop-types": 0,
    "react/no-danger": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "jsx-a11y/href-no-hash": "off"
  },
}
